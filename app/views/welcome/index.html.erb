  <div class="container">
    <div class="form-group">
      <h1>Welcome to Blocipedia</h1>
        <p>Where you can create and share Wikis!</p>
        <p>Created by: <%= link_to "Jackie", "https://www.facebook.com/profile.php?id=100009465465751" %></p>
    </div> 

  <% if current_user.present? %>
    <p><%= link_to "View Wikis", wikis_path %></p> or
    <p><%= link_to "Create New Wiki", new_wiki_path %></p>
  <% else %>
    <p><%= link_to "Sign In", new_user_session_path %> to your account </p> or <br>
    <p><%= link_to "Sign Up", new_user_registration_path %> to start creating and sharing Wikis.</p>
    <p><%= link_to "About Blocipedia", welcome_about_path %></p>
   <% end %> 
</div>
