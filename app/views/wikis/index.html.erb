<div class="container">
  <h1>Wikis</h1>
    <%= @wikis %>

    <% if policy(Wiki.new).create? %>
      <%= link_to "New Wiki", new_wiki_path %><br>
    <% end %> <%= @wiki.each do |wiki| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to wiki.title, wiki %>
      </h4>
    </div>
  </div>
<% end %>

    <%= link_to "Blocipedia", root_path %><br>
    <%= link_to "About", welcome_about_path %>
</div>
 
